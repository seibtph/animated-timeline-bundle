{
    "name": "pdir/animated-timeline-bundle",
    "type": "contao-bundle",
    "description": "Animated timeline bundle for Contao CMS",
    "license": "LGPL-3.0-or-later",
    "version": "0.0.4",
    "authors": [
        {
          "name": "pdir GmbH",
          "homepage": "https://pdir.de"
        },
        {
          "name": "Philipp Seibt",
          "homepage": "https://pdir.de",
          "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "^4.4"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Pdir\\AnimatedTimelineBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Pdir\\AnimatedTimelineBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pdir\\AnimatedTimelineBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/pdir/animated-timeline-bundle/issues",
        "source": "https://github.com/pdir/animated-timeline-bundle"
    },
    "suggest": {
        "contao-themes-net/mate-theme-bundle": "Animated timeline bundle can easily integrate into MATE theme.",
        "contao-themes-net/nature-theme-bundle": "Animated timeline bundle can easily integrate into NATURE theme.",
        "contao-themes-net/odd-theme-bundle": "Animated timeline bundle can easily integrate into ODD theme."
    }
}
